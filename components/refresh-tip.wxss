/* components/components.wxss */
.container {
  width:100%;
  height: 100vh;
  display:flex;
  align-items: center;
  justify-content:center;
  position:fixed;
  left: 0;
  top: 0;
  z-index: 2;
}
.container-wrap {
  width:240rpx;
  height: 90rpx;
  line-height: 90rpx;
  text-align:center;
  color: #fff;
  background-color:rgba(0, 0, 0, 0.8);
  font-size:30rpx;
  border-radius:16rpx;
}
/* pages/stack/bookList/search.wxss */
.search-wrap {
  width: 100%;
}
.custom-search {
  float:left;
  width: 80%!important;
}
.search-wrap .clear {
  float:right;
  width: 20%;
}
.search-wrap button{
  display:inline-block;
  width:80%;
  height: 80rpx;
  line-height:80rpx;
  padding: 0;
  margin: 0;
  border-color:#ccc;
  color: #ddd;
  border-radius: 12rpx;
  font-size: 28rpx;
}
.hover-btn {
  background-color: #ff7614;
}